{
  "name": "@project/controllers",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "typings": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "source": "./src/*.ts",
      "typings": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "barrels": "barrelsby --config .barrelsby.json",
    "build": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@tsed/ajv": "^7.64.0",
    "@tsed/common": "^7.64.0",
    "@tsed/core": "^7.64.0",
    "@tsed/di": "^7.64.0",
    "@tsed/engines": "^7.64.0",
    "@tsed/exceptions": "^7.64.0",
    "@tsed/json-mapper": "^7.64.0",
    "@tsed/logger": "^6.7.4",
    "@tsed/openspec": "^7.64.0",
    "@tsed/schema": "^7.64.0",
    "@tsed/swagger": "^7.64.0"
  },
  "devDependencies": {
    "barrelsby": "2.8.1",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vitest": "^1.3.1"
  }
}
