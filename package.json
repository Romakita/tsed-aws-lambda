{
  "name": "tsed-lambda",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "build:specs": "yarn run build && yarn tsed run generate-swagger --output ./specs",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage ",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit ",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install",
    "invoke:any": "yarn run build && yarn serverless invoke local -f any --data '{\"path\":\"/rest/hello-world\", \"httpMethod\": \"GET\"}'"
  },
  "dependencies": {
    "@tsed/ajv": "^7.63.2",
    "@tsed/cli": "^5.1.1",
    "@tsed/cli-core": "^5.1.1",
    "@tsed/cli-generate-swagger": "^5.1.1",
    "@tsed/common": "^7.63.2",
    "@tsed/di": "^7.63.2",
    "@tsed/engines": "^7.63.2",
    "@tsed/exceptions": "^7.63.2",
    "@tsed/json-mapper": "^7.63.2",
    "@tsed/logger": "^6.7.4",
    "@tsed/logger-file": "^6.7.4",
    "@tsed/openspec": "^7.63.2",
    "@tsed/platform-cache": "^7.63.2",
    "@tsed/platform-exceptions": "^7.63.2",
    "@tsed/platform-express": "^7.63.2",
    "@tsed/platform-log-middleware": "^7.63.2",
    "@tsed/platform-middlewares": "^7.63.2",
    "@tsed/platform-params": "^7.63.2",
    "@tsed/platform-response-filter": "^7.63.2",
    "@tsed/platform-serverless": "^7.63.2",
    "@tsed/platform-views": "^7.63.2",
    "@tsed/schema": "^7.63.2",
    "@tsed/swagger": "^7.63.2",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.3",
    "find-my-way": "^8.1.0",
    "method-override": "^3.0.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.3"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "5.1.1",
    "@tsed/cli-plugin-jest": "5.1.1",
    "@types/aws-lambda": "^8.10.136",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/method-override": "^0.0.35",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.27",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "yarn",
    "runtime": "node"
  }
}
